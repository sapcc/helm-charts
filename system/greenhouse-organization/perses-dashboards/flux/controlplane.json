      "0": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Controllers"
          },
          "plugin": {
            "kind": "StatChart",
            "spec": {
              "calculation": "last",
              "format": {
                "decimalPlaces": 0,
                "unit": "decimal"
              },
              "metricLabel": "",
              "thresholds": {
                "steps": [
                  {
                    "color": "blue",
                    "value": 0
                  },
                  {
                    "color": "red",
                    "value": 100
                  }
                ]
              }
            }
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "datasource": {
                      "kind": "PrometheusDatasource"
                    },
                    "minStep": "",
                    "query": "sum(go_info{namespace=\"$namespace\",pod=~\".*-controller-.*\"})",
                    "seriesNameFormat": "pods"
                  }
                }
              }
            }
          ]
        }
      },
      "1": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Max Work Queue"
          },
          "plugin": {
            "kind": "StatChart",
            "spec": {
              "calculation": "last-number",
              "format": {
                "unit": "seconds"
              },
              "metricLabel": "",
              "sparkline": {},
              "thresholds": {
                "steps": [
                  {
                    "color": "blue",
                    "value": 0
                  },
                  {
                    "color": "#EAB839",
                    "value": 50
                  },
                  {
                    "color": "red",
                    "value": 100
                  }
                ]
              }
            }
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "datasource": {
                      "kind": "PrometheusDatasource"
                    },
                    "minStep": "",
                    "query": "max(workqueue_longest_running_processor_seconds{namespace=\"$namespace\",pod=~\".*-controller-.*\"})",
                    "seriesNameFormat": "seconds"
