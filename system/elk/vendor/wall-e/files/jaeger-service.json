{
  "order": 0,
  "index_patterns": [
    "jaeger-service-*"
  ],
  "settings": {
    "index": {
      "mapping": {
        "nested_fields": {
          "limit": "50"
        }
      },
      "requests": {
        "cache": {
          "enable": "true"
        }
      },
      "number_of_shards": "3",
      "number_of_replicas": "1"
    }
  },
  "mappings": {
    "dynamic_templates": [
      {
        "span_tags_map": {
          "path_match": "tag.*",
          "mapping": {
            "ignore_above": 256,
            "type": "keyword"
          }
        }
      },
      {
        "process_tags_map": {
          "path_match": "process.tag.*",
          "mapping": {
            "ignore_above": 256,
            "type": "keyword"
          }
        }
      }
    ],
    "properties": {
      "operationName": {
        "ignore_above": 256,
        "type": "keyword"
      },
      "serviceName": {
        "ignore_above": 256,
        "type": "keyword"
      }
    }
  },
  "aliases": {}
}
